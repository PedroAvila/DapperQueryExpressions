<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>enable</Nullable>
	  <LangVersion>latest</LangVersion>
	<Authors>Pedro Ávila Núñez</Authors>
	<Description>Extensiones para Dapper con soporte de expresiones básicas. Requiere Dapper y Dapper.Contrib.</Description>
	<PackageId>PAN.DapperLambdaSQL</PackageId>
	<Title>PAN.DapperLambdaToSql</Title>
	<PackageProjectUrl>https://github.com/PedroAvila/DapperQueryExpressions</PackageProjectUrl>
	<RepositoryUrl>https://github.com/PedroAvila/DapperQueryExpressions</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Version>1.0.2</Version>
  </PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Dapper" Version="2.1.66">
			<PrivateAssets>all</PrivateAssets>
		</PackageReference>
		<PackageReference Include="Dapper.Contrib" Version="2.0.78">
			<PrivateAssets>all</PrivateAssets>
		</PackageReference>
	</ItemGroup>

	

</Project>
